#!/bin/sh
sudo apt-get update
sudo apt-get upgrade
sudo apt-get install apache2

echo '<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Storage</title>
  </head>
  <body>
    <img src="https://storage.cloud.google.com/sup_kykilebg/supdevinci-logo.png">
  </body>
</html>
| sudo tee /var/www/html/index.html
